{
  "name": "youtube-dl-gui",
  "productName": "youtube-dl gui",
  "description": "A youtube-dl graphical user interface",
  "version": "0.0.0",
  "author": "Yann Bertrand <github@yann-bertrand.fr>",
  "license": "MIT",
  "main": "app/background.js",
  "build": {
    "appId": "fr.yann-bertrand.youtube-dl-gui",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ]
  },
  "scripts": {
    "postinstall": "install-app-deps",
    "build": "gulp build",
    "prerelease": "gulp build --env=production",
    "release": "build",
    "start": "gulp start",
    "pretest": "gulp build-unit --env=test",
    "test": "electron-mocha app/specs.js.autogenerated --renderer --require source-map-support/register",
    "coverage": "npm test -- -R scripts/istanbul-reporter",
    "pree2e": "gulp build-e2e --env=test",
    "e2e": "mocha app/e2e.js.autogenerated --require source-map-support/register"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "datatables.net": "^3.2.2",
    "datatables.net-bs": "^3.2.2",
    "electron-config": "^0.2.1",
    "font-awesome": "^4.7.0",
    "fs-jetpack": "^0.10.2",
    "mkdirp": "^0.5.1",
    "pretty-bytes": "^4.0.2",
    "tether": "^1.4.0",
    "youtube-dl": "^1.11.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "electron": "^1.4.7",
    "electron-builder": "^8.6.0",
    "electron-mocha": "^3.0.0",
    "gulp": "^3.9.0",
    "gulp-batch": "^1.0.5",
    "gulp-less": "^3.0.3",
    "gulp-plumber": "^1.1.0",
    "gulp-util": "^3.0.6",
    "gulp-watch": "^4.3.5",
    "istanbul": "^0.4.3",
    "minimist": "^1.2.0",
    "mocha": "^3.0.2",
    "rimraf": "^2.5.4",
    "rollup": "^0.36.3",
    "rollup-plugin-istanbul": "^1.1.0",
    "source-map-support": "^0.4.2",
    "spectron": "^3.3.0"
  }
}
